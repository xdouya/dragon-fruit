<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="person.caiwenlao.modules.system.dao.DictDao" >
    <resultMap id="JoinResultMap" type="person.caiwenlao.modules.system.domain.po.DictPo" extends="BaseResultMap">
        <collection column="{dictId=dict_id}" property="dictDetails" select="person.caiwenlao.modules.system.dao.DictDetailDao.findByDictId"
    </resultMap>

    <select id="findByIdIn" resultMap="JoinResultMap">
        select
          *
        from
          sys_dict
        where
          dict_id
        in
        <foreach collection="list" item="dictId" open="（" close="）" separator=",">
            #{dictId, jdbcType=BIGINT}
        </foreach>
    </select>

    <delete id="deleteByIdIn">
        delete from
          sys_dict
        where
          dict_id
        in
        <foreach collection="list" item="dictId" open="（" close="）" separator=",">
            #{dictId, jdbcType=BIGINT}
        </foreach>
    </delete>
</mapper>